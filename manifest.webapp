{
	"name": "Foursquare",
	"description": "Simple Foursquare client",
	"version": "v1.0.2", 

	"launch_path": "/index.min.html",

	"developer": {
		"name": "Mathieu Rochette",
		"url": "https://mathieu.rochette.cc/"
	},

	"icons": {
		"128": "/images/place.png"
	},

	"default_locale": "en", 
	
	"type": "privileged", 
	
	"permissions": {
		"geolocation": {
			"description": "required to give quickly locate you"
		}, 
		"systemXHR": {
			"description": "required to upload photos"
		}
	}, 
	
	"redirects": [
		{
			"from": "http://place.texthtml.net/?authenticated", 
			"to" : "/index.min.html?authenticated"
		}
	]
}
